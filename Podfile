# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Skyhook' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Skyhook

  # Firebase pods
  pod 'Firebase'
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Messaging'
  
  # Image loading
 pod 'SDWebImage'
  
  # Pods for crash logging
  pod 'Fabric', '~> 1.10.2'
  pod 'Crashlytics', '~> 3.14.0'

  #GraphQL
  pod 'Apollo'
  
  #Close claim slider
  pod 'MTSlideToOpen'
  





  target 'SkyhookTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SkyhookUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
